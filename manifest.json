{
  "name": "autofillDB",
  "version": "1.0",
  "description": "An extension for autofilling interviewDB",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "matches": ["*://*.interview-db.com/*"],
    "js": ["jQuery.js", "content.js"]
  }],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}